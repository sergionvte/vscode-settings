{
  "vim.normalModeKeyBindingsNonRecursive": [
    {"before": ["<Space>"], "after": [":"]},
    {"before": ["/", "/"], "commands": [":nohl"]},
    {"before": [".", "."], "after": ["c", "i", "w"]},
    {"before": [",", ","], "after": ["c", "i", "w"]},
    {"before": ["w"],"after": ["e"]},
    {"before": ["e"], "after": ["w"]},
    {"before": ["ñ"], "after": ["$"]},
    {"before": ["Ñ"], "after": ["0"]},
    {"before": ["J"], "after": ["G"]},
    {"before": ["K"], "after": ["g", "g"]},
    {"before": ["n"], "after": ["N"]},
    {"before": ["N"], "after": ["#"]},
    {"before": ["H"], "after": ["0"]},
    {"before": ["L"], "after": ["$"] }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {"before": ["w"], "after": ["e"]},
    {"before": ["e"], "after": ["w"]},
    {"before": ["H"], "after": ["0"]},
    {"before": ["L"], "after": ["$"]},
    {"before": ["J"], "after": ["G"]},
    {"before": ["K"], "after": ["g", "g"]}
  ],
  "vim.handleKeys": {"<C-n>": false},
  "vim.useSystemClipboard": true,
  "vscode-pets.petSize": "medium",
  "vscode-pets.petColor": "white",
  "vscode-pets.petType": "rubber-duck",
  "vscode-pets.position": "explorer",
  "vscode-pets.throwBallWithMouse": true,
  "files.associations": {
  "**/templates/*.html": "django-html",
  "**/templates/*": "django-txt",
  "**/requirements{/**,*}.{txt,in}": "pip-requirements"},
  "symbols.hidesExplorerArrows": false,
  "emmet.includeLanguages": {"django-html": "html"},
  "terminal.integrated.fontSize": 14,
  "breadcrumbs.enabled": false,
  "files.trimTrailingWhitespace": true,
  "indenticator.color.dark": "rgba(255, 255, 255, 0.1)",
  "liveServer.settings.donotVerifyTags": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "oneDarkPro.vivid": true,
  "oneDarkPro.bold": true,
  "window.commandCenter": false,
  "window.title": "${folderName}",
  "editor.lineNumbers": "relative",
  "editor.fontFamily": "'FiraCode Nerd Font', 'Courier New', monospace",
  "editor.stickyScroll.enabled": false,
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "editor.overviewRulerBorder": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.guides.indentation": false,
  "editor.matchBrackets": "never",
  "editor.fontSize": 14,
  "editor.glyphMargin": false,
  "editor.occurrencesHighlight": "off",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.smoothScrolling": true,
  "workbench.sideBar.location": "right",
  "workbench.editor.enablePreview": false,
  "workbench.layoutControl.enabled": false,
  "workbench.editor.showTabs": "none",
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.iconTheme": "a-file-icon-vscode",
  "workbench.activityBar.location": "hidden",
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "One Dark Pro Flat",
  "workbench.preferredDarkColorTheme": "One Dark Pro Flat",
}